{
    "name": "dallas-makerspace/makermanager",
    "description": "DMS Membership Management System",
    "homepage": "http://dallasmakerspace.org",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": "~3.2",
        "mobiledetect/mobiledetectlib": "2.*",
        "cakephp/migrations": "~1.0",
        "cakephp/plugin-installer": "^1.3",
        "friendsofcake/bootstrap-ui": "^0.5.0",
        "ceeram/cakephp-blame": "~1.0",
        "php-coveralls/php-coveralls": "~1.0",
        "friendsofcake/cakephp-csvview": "~2.0",
        "directorytree/ldaprecord": "2.*"
    },
    "require-dev": {
        "cakephp/debug_kit": "~3.2",
        "cakephp/bake": "~1.1"
    },
    "suggest": {
        "phpunit/phpunit": "Allows automated tests to be run without system-wide install.",
        "cakephp/cakephp-codesniffer": "Allows to check the code against the coding standards used in CakePHP."
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "scripts": {
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "cakephp/plugin-installer": true
        }
    }
}
